<Window x:Class="Threads.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Threads"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<Button x:Name="btn_click" Content="Find prime number." HorizontalAlignment="Center" Margin="0,147,0,0" VerticalAlignment="Top" Click="btn_click_1"/>
		<TextBox x:Name="tb_output" HorizontalAlignment="Center" Margin="0,183,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
		<TextBox x:Name="tb_taskticks" HorizontalAlignment="Left" Margin="72,147,0,0" Text="Task ticks: " TextWrapping="Wrap" VerticalAlignment="Top" Width="173" Height="20"/>
		<TextBox x:Name="tb_threadticks" HorizontalAlignment="Left" Margin="72,179,0,0" Text="Thread ticks: " TextWrapping="Wrap" VerticalAlignment="Top" Width="173" Height="22"/>
	</Grid>
</Window>
